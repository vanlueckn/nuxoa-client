# Environment Variables for Integration Tests

# Required for all integration tests
NUXOA_CUSTOMER_ID=your_customer_id_here
NUXOA_API_KEY=your_api_key_here

# Cloud Server Tests
NUXOA_TEST_CLOUD_SERVER_ID=12345
NUXOA_TEST_SSH_KEY=ssh-rsa AAAAB3NzaC1yc2EAAAADAQABAAAB...
NUXOA_TEST_SNAPSHOT_ID=20240104120000
NUXOA_TEST_ORDER_SERVER=false  # Set to 'true' to actually order a server

# Dedicated Server Tests
NUXOA_TEST_DEDICATED_SERVER_ID=1
NUXOA_TEST_DEDICATED_CONTRACT_ID=67890

# IP Subnet Tests
NUXOA_TEST_IP_CONTRACT_ID=11111

# Domain Tests
NUXOA_TEST_REGISTER_DOMAIN=newdomain.com
NUXOA_TEST_TRANSFER_DOMAIN=transferredomain.com
NUXOA_TEST_TRANSFER_AUTHCODE=authcode123
NUXOA_TEST_MODIFY_DOMAIN=modifydomain.com
NUXOA_TEST_TRADE_DOMAIN=tradedomain.com
NUXOA_TEST_PASSWORD_DOMAIN=passworddomain.com
NUXOA_TEST_DELETE_DOMAIN=deletedomain.com

# DNS Tests
NUXOA_TEST_DNS_DOMAIN=example.com
NUXOA_TEST_DNS_RECORD_ID=record123
